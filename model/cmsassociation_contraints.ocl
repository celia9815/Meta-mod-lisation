import 'cmsassociation.ecore'

package cmsassociation

context Entite
--
-- example invariant with a custom error message to verify that
-- the 'id' property of all 'cmsassociation::Entite' instances is non-null
--
inv NonNull_id('The \'id\' property of "' + self.toString() + '" is null'):
	id <> null


context cmsassociation::Utilisateur
inv EmailValide:
  self.email.indexOf('.*@.*') >= 0 



context cmsassociation::Demande
inv AuMoinsUneReponse:
  self.reponses->size() >= 1



endpackage
